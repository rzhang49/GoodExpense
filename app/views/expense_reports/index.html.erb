    <td><%= expense_report.Cell_Phone %></td>
    <td><%= expense_report.Office_Phone %></td>
    <td><%= expense_report.Postage %></td>
    <td><%= expense_report.Office_Supplies %></td>
    <td><%= expense_report.Competitor_Products %></td>
    <td><%= expense_report.Date %></td>
    <td><%= expense_report.NameFirmPosition %></td>
    <td><%= expense_report.Purpose %></td>
    <td><%= link_to 'Show', expense_report %></td>
    <td><%= link_to 'Edit', edit_expense_report_path(expense_report) %></td>
    <td><%= link_to 'Destroy', expense_report, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
