    <td><%= expense_report.Hotel %></td>
    <td><%= expense_report.Other %></td>
    <td><%= expense_report.Breakfast %></td>
    <td><%= expense_report.Lunch %></td>
    <td><%= expense_report.Dinner %></td>
    <td><%= expense_report.Business_Meals %></td>
