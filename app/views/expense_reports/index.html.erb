<% @expense_reports.each do |expense_report| %>
  <tr>
    <td><%= expense_report.Employee_Name %></td>
    <td><%= expense_report.Division %></td>
    <td><%= expense_report.Department %></td>
    <td><%= expense_report.From %></td>
    <td><%= expense_report.To %></td>
    <td><%= expense_report.Business_Purpose %></td>
    <td><%= expense_report.Date %></td>
    <td><%= expense_report.Amount %></td>
    <td><%= expense_report.Airfare %></td>
    <td><%= expense_report.TaxiTransportation %></td>
    <td><%= expense_report.TollsParking %></td>
